{
  "name": "medicare-plus-healthcare-app",
  "version": "1.0.0",
  "description": "Comprehensive AI-integrated healthcare web application with real-time data synchronization and advanced health analytics",
  "main": "index.html",
  "scripts": {
    "start": "http-server -p 8000 -c-1",
    "dev": "http-server -p 8000 -c-1 --cors",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss css/dashboard-styles.css -o dist/css/styles.min.css",
    "build:js": "terser js/healthcare-backend-api.js js/ai-health-assistant.js js/dashboard-integration.js -o dist/js/app.min.js",
    "test": "node test/test-suite.js",
    "test:watch": "nodemon test/test-suite.js",
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,html,css,md}\"",
    "validate": "npm run lint && npm run test",
    "deploy": "npm run build && netlify deploy --prod",
    "deploy:vercel": "npm run build && vercel --prod",
    "backup": "node scripts/backup.js",
    "health-check": "node scripts/health-check.js",
    "setup": "node scripts/setup.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "docs": "jsdoc js/**/*.js -d docs/api",
    "analyze": "webpack-bundle-analyzer dist/js/app.min.js",
    "security-audit": "npm audit && snyk test",
    "performance": "lighthouse http://localhost:8000 --output html --output-path ./reports/lighthouse.html",
    "clean": "rimraf dist reports coverage",
    "precommit": "npm run lint && npm run test",
    "postinstall": "npm run setup"
  },
  "keywords": [
    "healthcare",
    "medical",
    "ai",
    "telemedicine",
    "patient-management",
    "health-analytics",
    "supabase",
    "deepseek",
    "real-time",
    "dashboard",
    "medication-management",
    "appointment-scheduling",
    "vital-signs",
    "health-insights"
  ],
  "author": {
    "name": "MediCare+ Development Team",
    "email": "dev@medicareplus.com",
    "url": "https://medicareplus.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/medicare-plus/healthcare-app.git"
  },
  "bugs": {
    "url": "https://github.com/medicare-plus/healthcare-app/issues"
  },
  "homepage": "https://medicareplus.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "chart.js": "^4.4.0",
    "moment": "^2.29.4",
    "crypto-js": "^4.2.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-html": "^7.1.0",
    "prettier": "^3.1.0",
    "terser": "^5.24.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "webpack-bundle-analyzer": "^4.9.1",
    "lighthouse": "^11.3.0",
    "snyk": "^1.1248.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0"
  },
  "optionalDependencies": {
    "sharp": "^0.32.6",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "workbox-cli": "^7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "html"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "no-undef": "error",
      "prefer-const": "error",
      "no-var": "error"
    },
    "globals": {
      "supabase": "readonly",
      "React": "readonly",
      "ReactDOM": "readonly",
      "Babel": "readonly"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,html,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "port": 8000,
    "host": "localhost",
    "ssl": false
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/medicare-plus"
  },
  "contributors": [
    {
      "name": "Healthcare Development Team",
      "email": "team@medicareplus.com",
      "role": "maintainer"
    }
  ],
  "files": [
    "html/",
    "css/",
    "js/",
    "images/",
    "docs/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "directories": {
    "doc": "docs",
    "test": "test",
    "example": "examples"
  },
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
} 
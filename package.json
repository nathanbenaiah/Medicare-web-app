{
  "name": "medicare-webapp",
  "version": "2.0.0",
  "description": "MediCare+ Advanced Healthcare Web Application with AI Assessment System, Machine Learning, and Health Document AI (inspired by Doctor Dok)",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "setup": "node setup-ai-features.js",
    "test-setup": "node test-setup.js",
    "train-models": "node scripts/train-cloud-models.js",
    "ml-health": "node scripts/check-ml-health.js",
    "ml-test": "node scripts/test-ml-predictions.js",
    "health-docs": "echo 'Health Document AI available at http://localhost:8000/html/health-documents.html'",
    "demo-health-ai": "node demo-health-doc-test.js"
  },
  "keywords": [
    "healthcare",
    "medicare",
    "ai-assessment",
    "machine-learning",
    "predictive-analytics",
    "cloud-ml",
    "health-documents",
    "medical-ocr",
    "fhir",
    "doctor-dok",
    "health-ai",
    "tesseract",
    "react",
    "nodejs",
    "pdf-generation",
    "mobile-responsive"
  ],
  "author": "MediCare+ Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "chart.js": "^4.5.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "deepseek-api": "^1.0.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "framer-motion": "^11.0.0",
    "helmet": "^7.1.0",
    "html-pdf": "^3.0.1",
    "html2canvas": "^1.4.1",
    "jimp": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "ml-matrix": "^6.10.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.9",
    "pdf2pic": "^3.2.0",
    "pdfkit": "^0.17.1",
    "puppeteer": "^21.9.0",
    "python-shell": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.12.0",
    "sharp": "^0.33.5",
    "simple-statistics": "^7.8.3",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.8",
    "tesseract.js": "^6.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "mlConfig": {
    "mode": "cloud",
    "fallbackMode": "statistical",
    "cacheTimeout": 600000,
    "trainingInterval": "weekly",
    "batchSize": 32,
    "confidenceThreshold": 0.7
  }
}
